{
    "root": true,
    "env": {
        "node": true,
        "browser": true
    },
    "parserOptions": {
        "ecmaVersion": 2019,
        "sourceType": "module",
        "parser": "babel-eslint"
    },
    "rules": {
        "strict": [
            "error",
            "never"
        ],
        "valid-typeof": "error",
        "no-unsafe-negation": "error",
        "no-unused-vars": "error",
        "no-unexpected-multiline": "error",
        "no-undef": "warn"
    },
    "extends": [
        "plugin:vue/vue3-essential",
        "eslint:recommended",
        "@vue/prettier"
    ]
}