{
  "name": "sharedrobes-calculator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "check-format": "npm run prettier -- --list-different",
    "check-types": "tsc",
    "deploy": "node scripts/gh-pages-deploy.js",
    "dev": "NODE_ENV=development vue-cli-service serve --open --host localhost",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|vue|ts|tsx)\"",
    "lint": "eslint --ignore-path .gitignore .",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json|vue|ts|tsx)\"",
    "serve": "vue-cli-service serve",
    "test": "jest",
    "validate": "npm run check-types && npm run check-format && npm run lint && npm run build"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-vue": "^7.0.0-0",
    "execa": "^5.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
